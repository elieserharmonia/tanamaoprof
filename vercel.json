
{
  "cleanUrls": true,
  "rewrites": [
    { 
      "source": "/sw.js", 
      "destination": "/sw.js" 
    },
    { 
      "source": "/manifest.json", 
      "destination": "/manifest.json" 
    },
    { 
      "source": "/((?!assets/|sw.js|manifest.json|.*\\.).*)", 
      "destination": "/index.html" 
    }
  ]
}
